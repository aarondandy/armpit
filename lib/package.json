{
  "name": "armpit",
  "version": "0.0.1",
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/esm/index.d.ts"
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "run-s -l clean build:*",
    "build:esm": "tsc",
    "build:cjs": "tsc -p ./tsconfig.cjs.json",
    "watch": "run-p -l watch:*",
    "watch:esm": "tsc -w",
    "watch:cjs": "tsc -w -p ./tsconfig.cjs.json",
    "lint": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "description": "Another resource manager programming interface toolkit.",
  "keywords": [
    "arm",
    "azure",
    "azure-cli",
    "az-cli"
  ],
  "author": "aarondandy",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/node": "^22.15.24",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@azure/arm-resources-subscriptions": "^2.1.0",
    "execa": "^9.5.2",
    "uuid": "^11.1.0"
  }
}
